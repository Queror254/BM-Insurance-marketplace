                    <!--two step UserAccountSetup form-->
                    <div class="form-content-container">
                        <div id="middle-wizard">
                            <form id="wrapped" method="POST" action="{{route('auto.quote')}}">
                                <input id="website" name="website" type="text" value="">
                                <!-- Leave for security protection, read docs for details -->

                                <div id="step-1" class="form-step">
                                    <h3 class="main_question"><strong>1/12</strong>Hi there! Tell us about yourself.
                                    </h3>
                                    <p>Please make sure it matches the information on your license.</p>
                                    <div class="form-group">
                                        <h6>First name</h6>
                                        <input type="text" name="firstname" id="firstname" class="form-control required"
                                            placeholder="eg. John">
                                    </div>
                                    <div class="form-group">
                                        <h6>Last name</h6>
                                        <input type="text" name="lastname" id="lastname" class="form-control required"
                                            placeholder="eg. Smith">
                                    </div>
                                    <div class="form-group">
                                        <h6>When's your birthday?</h6>
                                        <input type="text" name="dob" id="dob" class="form-control required"
                                            placeholder="MM/DD/YYYY">
                                        <h6 class="error-message" id="dobError"></h6>
                                    </div>
                                    <h6>Why we ask for this information?</h6>
                                    <p>Insurance companies use your age to recommend policies.
                                        Your answer will help us find more matched providers.
                                        Don’t worry. Your information is confidential.</p>

                                    <div class="form-group terms">
                                        <label class="container_check">Please accept our <a
                                                href="https://www.baronmeddy.com/terms-and-conditions/"
                                                data-bs-toggle="modal" data-bs-target="#terms-txt">Terms and
                                                conditions</a>
                                            <input type="checkbox" name="terms" value="Yes" class="required">
                                            <span class="checkmark"></span>
                                        </label>
                                    </div>
                                    <hr>
                                    <button class="button btn-navigate-form-step" type="button"
                                        step_number="2">Next</button>
                                </div>

                                <!--/step-->
                                <div id="step-2" class="form-step d-none">
                                    <h3 class="main_question"><strong>2/12</strong>What is your address?</h3>
                                    <div class="form-group">
                                        <p>We need your address to match you with insurance providers in your area</p>
                                        <h6>Enter your street address including suite or apt number</h6>
                                        <input type="text" id="streetaddress" name="streetaddress"
                                            class="form-control required" placeholder="eg. 123 Main St apt 1202">
                                    </div>
                                    <div class="form-group">
                                        <h6>City</h6>
                                        <input type="text" id="city" name="city" class="form-control required"
                                            placeholder="e.g: New York">
                                    </div>
                                    <div class="form-group">
                                        <h6>State</h6>
                                        <div class="styled-select clearfix">
                                            <input type="text" id="state" name="state" class="form-control required"
                                                placeholder="State" placeholder="enter your state">
                                        </div>
                                        <p> </p>
                                        <div class="form-group">
                                            <h6>Zip code</h6>
                                            <input type="text" name="zipCode" id="zipCode" class="form-control required"
                                                placeholder="eg. 20500">
                                        </div>
                                    </div>
                                    <div class="stepper-btns mt-4">
                                        <button class="button btn-navigate-form-step" type="button"
                                            step_number="1">Back</button>
                                        <button class="button btn-navigate-form-step" type="button"
                                            step_number="3">Next</button>
                                    </div>
                                </div>

                                <!--/step-->
                                <div id="step-3" class="form-step d-none">
                                    <h3 class="main_question"><strong>3/12</strong>Contact Information</h3>
                                    <p>We need your contact information to send you quotes.</p>
                                    <div class="form-group">
                                        <h6>What's the best phone number to reach you?</h6>
                                        <input type="text" id="phonenumber" name="phonenumber"
                                            class="form-control required" placeholder="eg. (123) 456-7890">
                                        <h6 class="error-message" id="phoneError"></h6>
                                    </div>
                                    <div class="form-group">
                                        <h6>What's your email address?</h6>
                                        <input type="text" name="email" id="email" class="form-control required"
                                            placeholder="eg. name@domain.com">
                                    </div>
                                    <div class="form-group">
                                        <h6>When is the best time to reach you?</h6>
                                        <div class="styled-select clearfix">
                                            <select class="wide required" name="contactTime" id="contactTime">
                                                <option value="">-Select an option-</option>
                                                <option value="morning">Morning between 9 am - 11:30 am</option>
                                                <option value="afternoon">Afternoon between 12 pm - 6 pm</option>
                                                <option value="evening">Evening between 6 pm - 9 pm</option>
                                            </select>
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <h6>What is your preferred way of contact?</h6>
                                        <div class="styled-select clearfix">
                                            <select class="wide required" name="preferredContactMethod"
                                                id="preferredContactMethod">
                                                <option value="">-Select an option-</option>
                                                <option value="email">Email</option>
                                                <option value="phonecall">Phone</option>
                                                <option value="email or phone">Either or is fine</option>
                                            </select>
                                        </div>
                                    </div>
                                    <div class="stepper-btns mt-4">
                                        <button class="button btn-navigate-form-step" type="button"
                                            step_number="2">Back</button>
                                        <button class="button btn-navigate-form-step" type="button"
                                            step_number="4">Next</button>
                                    </div>
                                </div>
                                <!-- /step-->

                                <!-- /step-->
                                <div id="step-4" class="form-step d-none">
                                    <h3 class="main_question"><strong>4/12</strong>Which of the following best describes
                                        where you live?</h3>
                                    <div class="form-group">
                                        <label class="container_radio version_2">I am a home owner
                                            <input type="radio" name="placeOfResidence" id="placeOfResidence"
                                                value="Completely Satisfied" class="required"
                                                onchange="getVals(this, 'placeOfResidence');">
                                            <span class="checkmark"></span>
                                        </label>
                                    </div>
                                    <div class="form-group">
                                        <label class="container_radio version_2">I currently rent
                                            <input type="radio" name="placeOfResidence" id="placeOfResidence"
                                                value="Completely Satisfied" class="required"
                                                onchange="getVals(this, 'placeOfResidence');">
                                            <span class="checkmark"></span>
                                        </label>
                                    </div>
                                    <div class="form-group">
                                        <label class="container_radio version_2">I live with parents/relatives
                                            <input type="radio" name="placeOfResidence" id="placeOfResidence"
                                                value="Completely Satisfied" class="required"
                                                onchange="getVals(this, 'placeOfResidence');">
                                            <span class="checkmark"></span>
                                        </label>
                                    </div>
                                    <div class="form-group">
                                        <label class="container_radio version_2">Other
                                            <input type="radio" name="placeOfResidence" id="placeOfResidence"
                                                value="Completely Satisfied" class="required"
                                                onchange="getVals(this, 'placeOfResidence');">
                                            <span class="checkmark"></span>
                                        </label>
                                    </div>
                                    <div class="stepper-btns mt-4">
                                        <button class="button btn-navigate-form-step" type="button"
                                            step_number="3">Back</button>
                                        <button class="button btn-navigate-form-step" type="button"
                                            step_number="5">Next</button>
                                    </div>
                                </div>
                                <!-- /step-->
                                <div id="step-5" class="form-step d-none">
                                    <h3 class="main_question"><strong>5/12</strong>What’s your main goal here today?
                                    </h3>
                                    <p>This won’t impact your application at all — it just helps us get to know you
                                        better.
                                    </p>

                                    <div class="form-group">
                                        <label class="container_radio version_2">I want to get insurance coverage today
                                            (or
                                            soon)
                                            <input type="radio" name="intention" id="intention" value="Quite Satisfied"
                                                class="required" onchange="getVals(this, 'intention');">
                                            <span class="checkmark"></span>
                                        </label>
                                    </div>
                                    <div class="form-group">
                                        <label class="container_radio version_2">I’m comparing Baron Meddy to other
                                            quotes
                                            I’ve gotten
                                            <input type="radio" name="intention" id="intention"
                                                value="I’m comparing Ladder to other quotes I’ve gotten"
                                                class="required" onchange="getVals(this, 'intention');">
                                            <span class="checkmark"></span>
                                        </label>
                                    </div>
                                    <div class="form-group">
                                        <label class="container_radio version_2">I'm here to see my price
                                            <input type="radio" name="intention" id="intention" value="Satisfied"
                                                class="required" onchange="getVals(this, 'intention');">
                                            <span class="checkmark"></span>
                                        </label>
                                    </div>
                                    <div class="form-group">
                                        <label class="container_radio version_2">Other
                                            <input type="radio" name="intention" id="intention"
                                                value="Extremely Satisfied" class="required"
                                                onchange="getVals(this, 'intention');">
                                            <span class="checkmark"></span>
                                        </label>
                                    </div>
                                    <div class="stepper-btns mt-4">
                                        <button class="button btn-navigate-form-step" type="button"
                                            step_number="4">Back</button>
                                        <button class="button btn-navigate-form-step" type="button"
                                            step_number="6">Next</button>
                                    </div>
                                </div>

                                <!-- /step-->
                                <div id="step-6" class="form-step d-none">
                                    <h3 class="main_question"><strong>6/12</strong> Thanks, now let’s add your first
                                        vehicle.</h3>
                                    <h6>Make</h6>
                                    <div class="form-group">
                                        <div class="styled-select clearfix">
                                            <select class="wide required" name="vehicleMake" id="vehicleMake">
                                                <option value="">-Select-</option>
                                                <option value="Acura">Acura</option>
                                                <option value="Alfa Romeo">Alfa Romeo</option>
                                                <option value="Aston Martin">Astin Martin</option>
                                                <option value="Audi">Audi</option>
                                                <option value="BMW">BMW</option>
                                                <option value="Bentley">Bentley</option>
                                                <option value="Bollinger">Bollinger</option>
                                                <option value="Bugatti">Bugatti</option>
                                                <option value="Buick">Buick</option>
                                                <option value="Cadillac">Cadillac</option>
                                                <option value="Canoo">Canoo</option>
                                                <option value="Chevrolet">Chevrolet</option>
                                                <option value="Chrysler">Chrysler</option>
                                                <option value="Delorean">Delorean</option>
                                                <option value="Dodge">Dodge</option>
                                                <option value="Ferrari">Ferrari</option>
                                                <option value="Fiat">Fiat</option>
                                                <option value="Fisker">Fisker</option>
                                                <option value="Ford">Ford</option>
                                                <option value="GMC">GMC</option>
                                                <option value="Genesis">Genesis</option>
                                                <option value="Honda">Honda</option>
                                                <option value="Hummer">Hummer</option>
                                                <option value="Hyundai">Hyundai</option>
                                                <option value="Ineos">Ineos</option>
                                                <option value="Infiniti">Infiniti</option>
                                                <option value="Jaguar">Jaguar</option>
                                                <option value="Jeep">Jeep</option>
                                                <option value="Karma">Karma</option>
                                                <option value="Kia">Kia</option>
                                                <option value="Lamborghini">Lamborghini</option>
                                                <option value="Land Rover">Land Rover</option>
                                                <option value="Lexus">Lexus</option>
                                                <option value="Lincol">Lincoln</option>
                                                <option value="Lordstown">Lordstown</option>
                                                <option value="Lotus">Lotus</option>
                                                <option value="Lucid Motors">Lucid Motors</option>
                                                <option value="Maserati">Maserati</option>
                                                <option value="Maybach">Maybach</option>
                                                <option value="Mazda">Mazda</option>
                                                <option value="McLaren">McLaren</option>
                                                <option value="Mercedes-AMG">Mercedes-AMG</option>
                                                <option value="Mercedes-Benz">Mercedes-Benz</option>
                                                <option value="Mercury">Mercury</option>
                                                <option value="Mini">Mini</option>
                                                <option value="Mitsubishi">Mitsubishi</option>
                                                <option value="Nissan">Nissan</option>
                                                <option value="Oldsmobile">Oldsmobile</option>
                                                <option value="Polestar">Polestar</option>
                                                <option value="Pontiac">Pontiac</option>
                                                <option value="Porsche">Porsche</option>
                                                <option value="Plymouth">Plymouth</option>
                                                <option value="Ram">Ram</option>
                                                <option value="Ratis">Ratis</option>
                                                <option value="Rivian">Rivian</option>
                                                <option value="Plymouth">Plymouth</option>
                                                <option value="Rolls-Royce">Rolls-Royce</option>
                                                <option value="Saab">Saab</option>
                                                <option value="Saturn">Saturn</option>
                                                <option value="Scion">Scion</option>
                                                <option value="Subaru">Subaru</option>
                                                <option value="Suzuki">Suziki</option>
                                                <option value="Tesla">Tesla</option>
                                                <option value="Toyota">Toyota</option>
                                                <option value="Volkswagen">Volkswagen</option>
                                                <option value="VinFast">VinFast</option>
                                                <option value="Volvo">Volvo</option>
                                            </select>
                                        </div>
                                        <p> </p>
                                        <h6>Model</h6>
                                        <div class="form-group">
                                            <input type="text" name="vehicleModel" id="vehicleModel"
                                                class="form-control required" placeholder="eg. Altima">
                                        </div>
                                        <p> </p>
                                        <div class="form-group">
                                            <input type="number" name="vehicleYear" id="vehicleYear"
                                                class="form-control required" placeholder="Year">
                                        </div>
                                        <p> </p>
                                        <h3 class="main_question"><strong></strong>Do you currently have an auto
                                            insurance
                                            policy?</h3>
                                        <div class="form-group">
                                            <label class="container_radio version_2">Yes
                                                <input type="radio" name="haveVehicleInsurance"
                                                    id="haveVehicleInsurance" value="Completely Satisfied"
                                                    class="required" onchange="getVals(this, 'question_1');">
                                                <span class="checkmark"></span>
                                            </label>
                                        </div>
                                        <div class="form-group">
                                            <label class="container_radio version_2">No
                                                <input type="radio" name="haveVehicleInsurance"
                                                    id="haveVehicleInsurance" value="Completely Satisfied"
                                                    class="required" onchange="getVals(this, 'question_1');">
                                                <span class="checkmark"></span>
                                            </label>
                                        </div>
                                        <div class="stepper-btns mt-4">
                                            <button class="button btn-navigate-form-step" type="button"
                                                step_number="5">Back</button>
                                            <button class="button btn-navigate-form-step" type="button"
                                                step_number="7">Next</button>
                                        </div>
                                    </div>
                                </div>

                                <!-- /step-->
                                <div id="step-7" class="form-step d-none">
                                    <h3 class="main_question"><strong>7/12</strong> Just a little more infromation about
                                        your vehicle.</h3>
                                    <h6>Does your vehicle have any safety feature?</h6>
                                    <p>Select all that applies</p>
                                    <div class="form-group">
                                        <label class="container_check version_2">Air bag
                                            <input type="checkbox" name="safetyFeature[]" id="safetyFeature[]"
                                                value="Google Search Engine" class="required"
                                                onchange="getVals(this, 'question_2');">
                                            <span class="checkmark"></span>
                                        </label>
                                    </div>
                                    <div class="form-group">
                                        <label class="container_check version_2">Automated parking
                                            <input type="checkbox" name="safetyFeature[]" id="safetyFeature[]"
                                                value="Google Search Engine" class="required"
                                                onchange="getVals(this, 'question_2');">
                                            <span class="checkmark"></span>
                                        </label>
                                    </div>
                                    <div class="form-group">
                                        <label class="container_check version_2">Backup camera
                                            <input type="checkbox" name="safetyFeature[]" id="safetyFeature[]"
                                                value="Google Search Engine" class="required"
                                                onchange="getVals(this, 'question_2');">
                                            <span class="checkmark"></span>
                                        </label>
                                    </div>
                                    <div class="form-group">
                                        <label class="container_check version_2">Blindspot warning
                                            <input type="checkbox" name="safetyFeature[]" id="safetyFeature[]"
                                                value="Google Search Engine" class="required"
                                                onchange="getVals(this, 'question_2');">
                                            <span class="checkmark"></span>
                                        </label>
                                    </div>
                                    <div class="form-group">
                                        <label class="container_check version_2">Lane change assist
                                            <input type="checkbox" name="safetyFeature[]" id="safetyFeature[]"
                                                value="Google Search Engine" class="required"
                                                onchange="getVals(this, 'question_2');">
                                            <span class="checkmark"></span>
                                        </label>
                                    </div>
                                    <p> </p>
                                    <div class="form-group">
                                        <h6>Automobile VIN</h6>
                                        <input type="text" name="automobileVin" id="automobileVin"
                                            class="form-control required" placeholder="eg. 4Y1SL65848Z411439">
                                    </div>
                                    <div class="stepper-btns mt-4">
                                        <button class="button btn-navigate-form-step" type="button"
                                            step_number="6">Back</button>
                                        <button class="button btn-navigate-form-step" type="button"
                                            step_number="8">Next</button>
                                    </div>
                                </div>

                                <!-- /step-->
                                <div id="step-8" class="form-step d-none">
                                    <h3 class="main_question"><strong>8/12</strong> Vehicle usage</h3>
                                    <h6>Primary use of the vehicle</h6>
                                    <div class="styled-select clearfix">
                                        <select class="wide required" name="vehicleUsage" id="vehicleUsage">
                                            <option value="">-Select-</option>
                                            <option>Personal/Commuting</option>
                                            <option>Pleasure</option>
                                            <option>Business/rideshare</option>
                                        </select>
                                    </div>
                                    <p> </p>
                                    <h6>How many drivers will be included in this policy?</h6>
                                    <div class="styled-select clearfix">
                                        <select class="wide required" name="Drivers" id="Drivers">
                                            <option value="">-Select-</option>
                                            <option>One (myself)</option>
                                            <option>Two</option>
                                            <option>Three</option>
                                            <option>Four</option>
                                            <option>Five or more</option>
                                        </select>
                                    </div>

                                    <p></p>
                                    <div class="form-group">
                                        <h6>Estimated annual mileage per year?</h6>
                                        <input type="text" name="mileage" id="mileage" class="form-control required"
                                            placeholder="eg. 3500">
                                    </div>
                                    <div class="stepper-btns mt-4">
                                        <button class="button btn-navigate-form-step" type="button"
                                            step_number="7">Back</button>
                                        <button class="button btn-navigate-form-step" type="button"
                                            step_number="9">Next</button>
                                    </div>
                                </div>

                                <!-- /step-->
                                <div id="step-9" class="form-step d-none">
                                    <h3 class="main_question"><strong>9/12</strong> Driving History</h3>
                                    <h6>When was your license issued?</h6>
                                    <p> </p>
                                    <div class="form-group">
                                        <input type="text" name="licenseIssueDate" id="licenseIssueDate"
                                            class="form-control required" placeholder="eg. 09/10/2021">
                                    </div>
                                    <p> </p>
                                    <h6>Any accidents or claims in the past three years?</h6>
                                    <div class="form-group">
                                        <label class="container_radio version_2">Yes
                                            <input type="radio" name="accidentHistory" id="accidentHistory"
                                                value="Completely Satisfied" class="required"
                                                onchange="getVals(this, 'accidentHistory');">
                                            <span class="checkmark"></span>
                                        </label>
                                    </div>
                                    <div class="form-group">
                                        <label class="container_radio version_2">No
                                            <input type="radio" name="accidentHistory" id="accidentHistory"
                                                value="Completely Satisfied" class="required"
                                                onchange="getVals(this, 'accidentHistory');">
                                            <span class="checkmark"></span>
                                        </label>
                                    </div>
                                    <div class="form-group">
                                        <label>If yes, please describe below (we know, life happens)</label>
                                        <textarea name="accidentDesc" id="accidentDesc" class="form-control"
                                            style="height:100px;" placeholder="Type your message here..."
                                            onkeyup="getVals(this, 'accidentDesc');"></textarea>
                                    </div>
                                    <div class="stepper-btns mt-4">
                                        <button class="button btn-navigate-form-step" type="button"
                                            step_number="8">Back</button>
                                        <button class="button btn-navigate-form-step" type="button"
                                            step_number="10">Next</button>
                                    </div>
                                </div>

