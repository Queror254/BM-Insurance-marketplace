                    <div class="chat-container">

                        <label class="filter-type" for="filter">Deductible</label>

                        <canvas id="priceChart" width="400" height="200"></canvas>
                        <input type="range" id="priceSlider" min="0" value="0">

                        <div class="space"></div>

                        <!-- Price Range Filter -->
                        <div class="price-range-filter d-flex mb-10 ">
                            <div class="mr-2">
                                <div class="custom-input-container">
                                    <label class="filter" for="minPrice">MINIUMUM
                                        PRICE</label>
                                    <input type="number" id="minPrice" class="form-control" value="$"
                                        placeholder="$0.00">
                                </div>
                            </div>
                            <div class="mr-2">
                                <hr />
                            </div>
                            <div>
                                <div class="custom-input-container">
                                    <label class="filter" for="maxPrice">MAXIMUM
                                        PRICE</label>
                                    <input type="number" id="maxPrice" class="form-control" placeholder="$0.00">
                                </div>
                            </div>
                        </div>

                    </div>

                    <div class="space"></div>


                    <!-- Add your filter options here -->
                    <label class="filter-type" for="filter">Plan type</label>
                    <div class="options ">
