                <div class="s-content">

                    <!--filter-card start-->
                    <div class="filter-card mt-4">
                        <form method="POST" action="/filter">
                            <div class="filter-option mt-2">
                                <div class="custom-select-container">
                                    <label for="insuranceType">INSURANCE TYPE:</label>
                                    <select id="insuranceType" name="insuranceType" class="form-control">
                                        <option value="health">Health Insurance</option>
                                        <option value="auto">Auto Insurance</option>
                                        <option value="business">Business Insurance</option>
                                        <option value="life">Life Insurance</option>
                                        <option value="homeowner">Homeowner Insurance</option>
                                        <option value="renter">Renters Insurance</option>
                                        <!-- Add more options as needed -->
                                    </select>
                                </div>
                            </div>

                            <div class="filter-option mt-2">
                                <div class="custom-select-container">
                                    <label for="age">AGE:</label>
                                    <select id="age" name="age" class="form-control">
                                        <option value="18to25">18-25</option>
                                        <option value="26to40">26-40</option>
                                        <option value="40to60">40-60</option>
                                        <!-- Add more options as needed -->
                                    </select>
                                </div>
                            </div>

                            <div class="filter-option mt-2">
                                <div class="custom-input-container">
                                    <label for="zipcode">ZIPCODE:</label>
                                    <input type="number" id="zipCode" name="zipCode" class="form-control"
                                        placeholder="10005">
                                </div>
                            </div>

                            <div class="search-option mt-3">
                                <button type="submit" class="search-btn">
                                    <i class="fas fa-search"></i> Search
                                </button>
                            </div>

                            <div class="sort-filter">
                                <label for="sortByPrice" class="sortByPrice mt-2">
                                    Sort by Price:

                                </label>
                                <select id="priceOrder" name="priceOrder" class="form-control mt-1">
                                    <option value="lowToHigh">Low to High</option>
                                    <option value="highToLow">High to Low</option>
                                    <!-- Add more options as needed -->
                                </select>
                            </div>

                        </form>

